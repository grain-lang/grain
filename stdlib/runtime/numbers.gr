@unsafe
let safeI64Multiply = (x, y) => {
  from WasmI64 use { (!=), (*), (/) }
  let prod = x * y
  if (x != 0N) {
    if (prod / x != y) {
      let xBig = BI.makeWrappedInt64(x)
      let yBig = BI.makeWrappedInt64(y)
      let result = BI.mul(xBig, yBig)
      Memory.decRef(xBig)
      Memory.decRef(yBig)
      result
    } else {
      reducedInteger(prod)
    }
  } else {
    reducedInteger(prod)
  }
}

