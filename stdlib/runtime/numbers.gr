    let tag = boxedNumberTag(x)
    if (
      tag == Tags._GRAIN_INT64_BOXED_NUM_TAG
      || tag == Tags._GRAIN_BIGINT_BOXED_NUM_TAG
      || tag == Tags._GRAIN_RATIONAL_BOXED_NUM_TAG
