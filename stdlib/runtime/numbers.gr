  let x = WasmRef.fromGrain(number)
  let result = if (isSimpleNumber(x)) {
    newRational(BI.makeWrappedInt32(untagSimple(x)), BI.makeWrappedInt32(1n))
  } else {
    let tag = boxedNumberTag(x)
    if (tag == Tags._GRAIN_RATIONAL_BOXED_NUM_TAG) {
