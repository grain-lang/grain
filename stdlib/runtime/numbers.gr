          let denominator = BI.mul(xDenominator, y)
          reducedFractionBigInt(xNumerator, denominator, false)
        } else {
          // (a / b) * y == (a * y) / b
          let numerator = BI.mul(xNumerator, y)
          reducedFractionBigInt(numerator, xDenominator, false)
        }
