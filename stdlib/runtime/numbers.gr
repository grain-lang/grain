  } else {
    let yBoxedNumberTag = boxedNumberTag(y)
    match (yBoxedNumberTag) {
      t when t == Tags._GRAIN_INT64_BOXED_NUM_TAG => {
        let yBoxedVal = boxedInt64Number(y)
        if (isDivide) {
