(jbuild_version 1)

(library
 ((name grain)
  (public_name grain)
  ;; Note: 'dyp' is provided by the 'dypgen' OPAM package
  (libraries   (oUnit extlib batteries cmdliner ocamlgraph wasm stdint grain_parsing grain_utils grain_typed))))

(install
 ((section lib)
  (files ((grain-stdlib/lists.grlib as stdlib/lists.grlib)))
  (package grain)))
