{
  "name": "@grain/compiler",
  "private": true,
  "version": "0.3.2",
  "bin": {
    "grainc": "_esy/default/build/install/default/bin/grainc"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "esy": "0.6.10"
  },
  "scripts": {
    "link": "yarn link",
    "clean": "esy clean",
    "prepare": "esy install",
    "build": "esy",
    "postbuild": "esy copy:exe",
    "build:js": "esy b dune build --no-buffer --profile jsoo",
    "postbuild:js": "esy copy:js",
    "pretest": "esy && del-cli 'test/input/*.wasm' 'test/input/*.wat' 'test/input/*.modsig' 'test/output/*.wasm' 'test/output/*.wat' 'test/output/*.modsig' 'test/test-libs/*.wasm' 'test/test-libs/*.wat' 'test/test-libs/*.modsig'",
    "test": "esy test",
    "format": "esy format",
    "check-format": "esy check-format",
    "import-dependencies": "esy import-dependencies _export",
    "export-dependencies": "esy export-dependencies",
    "build-dependencies": "esy build-dependencies"
  }
}
