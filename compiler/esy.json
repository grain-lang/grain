{
  "name": "compiler",
  "version": "0.0.0",
  "esy": {
    "build": "dune build @all @install",
    "buildEnv": {
      "DUNE_BUILD_DIR": "#{self.target_dir}",
      "GRAIN_STDLIB": "#{self.root}/../stdlib"
    }
  },
  "scripts": {
    "clean": "rm -rf #{self.root}/_esy",
    "install-compiler": "dune install",
    "test": "dune runtest --force"
  },
  "dependencies": {
    "ocaml": "~4.8.1000",
    "@opam/ocamlfind": ">= 1.7.3",
    "@opam/ocamlbuild": ">= 0.12.0",
    "@opam/conf-openssl": ">= 2",
    "@opam/dune": ">= 2.0",
    "@opam/ppx_deriving": "*",
    "@opam/ppx_deriving_yojson": "*",
    "@opam/yojson": "*",
    "@opam/ppx_sexp_conv": "*",
    "@opam/sexplib": "*",
    "@opam/grain_dypgen": "0.1",
    "@opam/extlib": ">= 1.7.2",
    "@opam/batteries": ">= 2.8.0",
    "@opam/core": ">= 0.11.2",
    "@opam/cmdliner": ">= 1.0.2",
    "@opam/ocamlgraph": ">= 1.8.8",
    "@opam/grain_wasm_spec": "0.1",
    "@opam/stdint": ">= 0.5.0"
  },
  "devDependencies": {
    "@opam/ounit": ">= 2.0.0"
  },
  "installConfig": {
    "pnp": false
  }
}
