// Contrived example to test simple tail calls

let rec isEvenHelp = (n, b) => {
  if (n == 0) {
    b
  } else {
    isOddHelp(n - 1, !b)
  }
},

isOddHelp = (n, b) => {
  if (n == 1) {
    b
  } else {
    isEvenHelp(n - 1, !b)
  }
};

let isEven = (n) => {isEvenHelp(n, true)},
  isOdd = (n) => {isOddHelp(n, true)};

print(isOdd(9999))
