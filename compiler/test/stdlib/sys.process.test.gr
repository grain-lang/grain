module ProcessTest

from "array" include Array
from "sys/process" include Process

// Just a smoke test
match (Process.argv()) {
  Ok(arr) => assert Array.length(arr) > 0,
  Err(err) => throw err,
}

// Just a smoke test
match (Process.env()) {
  Ok(arr) => assert Array.length(arr) == 0,
  Err(err) => throw err,
}

Process.exit(5)
