{
  "name": "@grain/cli",
  "version": "0.3.2",
  "description": "A command line tool for the Grain language.",
  "main": "index.js",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "link": "yarn link",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-pkg": "pkg ./package.json --no-bytecode --compress Brotli --output ../pkg/grain"
  },
  "pkg": {
    "assets": "bin/*.js"
  },
  "bin": {
    "grain": "bin/grain.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grain-lang/grain.git"
  },
  "keywords": [
    "grain",
    "cli"
  ],
  "author": "Oscar Spencer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grain-lang/grain/issues"
  },
  "homepage": "https://github.com/grain-lang/grain#readme",
  "dependencies": {
    "@grain/js-runner": "^0.3.0",
    "@grain/stdlib": "^0.3.2",
    "commander": "^7.2.0"
  },
  "devDependencies": {
    "pkg": "^5.3.1",
    "prettier": "^2.2.1"
  }
}
